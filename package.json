{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:prettier": "npx prettier --list-different \"*{.js,.json}\" \"**/*{.ts,.tsx,.json,.css,.scss,.html}\"",
    "lint:prettier:fix": "npx prettier --write \"*{.js,.json}\" \"**/*{.ts,.tsx,.json,.css,.scss,.html}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "13.4.3",
    "next-seo": "^5.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.6.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "18.15.10",
    "@types/react": "18.0.30",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "^10.4.14",
    "eslint": "8.36.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.0",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint",
    "*.{html,json}": "prettier --list-different"
  }
}
